<div class="row exp-lookit-survey">
    <div class="col-md-12 exp-lookit-survey-form">
        {{dynamic-form schema=formSchema postRender=(action 'setupForm')}}
    </div>
</div>

<div class="row exp-controls exp-lookit-survey">
    <div class="col-md-12">
    {{#if showPreviousButton}}
        <button class="btn btn-default pull-left" {{ action 'previous' }}>
            Previous
        </button>
    {{/if}}

    <button id="nextbutton" class="btn btn-success pull-right" {{ action 'finish' }}>
       {{exp-format nextButtonText}}
    </button>
    </div>
</div>

